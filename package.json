{
  "private": true,
  "name": "squoosh",
  "version": "0.0.0",
  "license": "apache-2.0",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
